- xero = XeroClientService::XeroApi.new(XeroClientService.create_xero)
- data = xero.get_online_invoice('88e1a878-a697-4978-bb74-4a0f5dda042c')
= debug data

.page-header
  %h1.pull-left
    %i.icon-cogs
    %span Oauth Tokens
  .pull-right
    %a.btn.btn-white{ :href => '/oauth_tokens/new' }= t('formtastic.actions.new')

.row-fluid
  .box-content.box-no-padding
    .responsive-table
      .scrollable-area
        %table.table.table-striped{ :style => "margin-bottom:0;" }
          %thead
            %tr
              %th Issuer
              %th Tenant Id
              %th Created At
              %th Updated At

          %tbody
            - @oauth_tokens.each do |oauth_token|
              %tr
                %td= oauth_token.issuer
                %td= oauth_token.tenant_id
                %td= oauth_token.created_at
                %td= oauth_token.updated_at

