%p#notice= notice

%p
  %b Name:
  = @event_type.name
%p
  %b Subtitle:
  = @event_type.subtitle
%p
  %b Goal:
  = @event_type.goal
%p
  %b Description:
  = @event_type.description
%p
  %b Recipients:
  = @event_type.recipients
%p
  %b Program:
  = @event_type.program
%p
  %b Preguntas Frecuentes:
  = @event_type.faq
%p
  %b Materiales:
  = @event_type.materials
%p
  %b External site url:
  = @event_type.external_site_url
%p
  %b Canonical:
  - unless @event_type.canonical.nil?
    = link_to @event_type.canonical.name, @event_type.canonical
%p
  %b Clons:
  - @event_type.clons.each do |et|
    = link_to et.name, et


= link_to 'Edit', edit_event_type_path(@event_type)
\|
= link_to 'Back', event_types_path
